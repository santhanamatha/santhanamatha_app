{
  "version": 2,
  "builds": [
    {
      "src": "santhanamatha/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "60xmb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "santhanamatha/wsgi.py"
    }
  ]
}